package ab;

import java.lang.*;
import java.util.*;
  
class Main{

static int minSwap(int arr[], int n, int k) {
  
	for(int l = 0;l<n;l++) {
		if(n<1&& n>Math.pow(10, 5)) {
			System.out.println("Constraints are not followed");
		}
		else if(arr[l]>1048 && arr[l]<-1048) {
			System.out.println("Constraints are not followed");
			
		}
		else if(k<-1048 && k>1048) {
			System.out.println("Constraints are not followed");
		}
	}
    int count = 0;
    for (int i = 0; i < n; ++i)
    if (arr[i] <= k)
        ++count;
  
    int unwanted = 0;
    for (int i = 0; i < count; ++i)
    if (arr[i] > k)
        ++unwanted;
  
    int ans = unwanted;
    for (int i = 0, j = count; j < n; ++i, ++j) {
  
 
    if (arr[i] > k)
        --unwanted;
  
    if (arr[j] > k)
        ++unwanted;
  
    ans = Math.min(ans, unwanted);
    }
    return ans;
}

  

public static void main(String[] args) 
{
    Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
	    int arr[] = new int[n];
	    for(int i = 0;i <n;i++) {
	    	arr[i] = sc.nextInt();
	    }
	    
	    int k = sc.nextInt();
	   System.out.println(minSwap(arr,n,k));
}
}