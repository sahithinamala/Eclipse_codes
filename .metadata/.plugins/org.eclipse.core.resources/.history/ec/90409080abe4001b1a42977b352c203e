package ab;

import java.util.Scanner;

public class decimal_to_binary {
	static int dec2bin(int n) {
		int num = n;
		int bin=0;
		for(int i =0; i <= num ; i++) {
			int rem = n%2;
			num = num/2;
			bin = bin+rem*10;
			i=i*10;
		}
		return bin;
		
	}
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
		int n = sc.nextInt();
		
		System.out.println(dec2bin(n));
	}

}
