package ab;

import java.util.*;
public class Pranav_Harshith {
    public static void main(String[]args){
        Scanner sc = new Scanner(System.in);
        System.out.println("Enter the array size");
        int n = sc.nextInt();
        System.out.println("Enter the elements in the array");
        int array[] = new int[n];
        for(int i = 0; i<=n-1 ; i++) {
        array[i] = sc.nextInt();
        }
        int temp = 0;
        for(int i = 0;i <= n; i++){
            for(int j = i+1; j <= n ; j++){
                if(array[i]>array[j]){
                    temp = array[i];
                    array[i] = array[j];
                    array[j] = temp;
                }
                
                
            }
        }
        System.out.println();
        
        for(int i =0; i<=n;i++){
            System.out.println(array[i]);
        }
    }
}

