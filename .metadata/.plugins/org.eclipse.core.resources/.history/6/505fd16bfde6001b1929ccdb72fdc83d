package ab;
import java.util.*;
public class MissingNumber {
	static void missing(int arr[], int n) {
		
		int count = 1;
		for(int i = 0;i<n;i++) {
			if(arr[i] == count) {
				count++;
			}
			else {
				System.out.println(count + " is missing");
				count++;
				for(int j = 1;j<=n;j++)
				if(count == arr[i]) {
					count = count +j;
				}
			}
		}
	}
	public static void main(String[]args) {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the size of an array");
		int n = sc.nextInt();
		int arr[] = new int[n];
		System.out.println("Enter the elements");
		for(int i =0; i<n; i++) {
			arr[i] = sc.nextInt();
		}
		missing(arr,n);
	}

}
