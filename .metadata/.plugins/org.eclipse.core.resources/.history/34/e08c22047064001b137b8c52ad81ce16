package DAA;

import java.util.*;
public class Array
{  
public static void main(String[] args)   
{  
	
 Scanner sc = new Scanner(System.in);
 int n = sc.nextInt();
int[] arr = new int[n];  
System.out.println("Array elements after sorting:");  

for (int i = 0; i < arr.length; i++)   
{  
for (int j = i + 1; j < arr.length; j++)   
{  
int tmp = 0;  
if (arr[i] > arr[j])   
{  
tmp = arr[i];  
arr[i] = arr[j];  
arr[j] = tmp;  
}  
}  
 
System.out.print(arr[i] + " ");  
}  
}  
}  